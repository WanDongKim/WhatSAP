@model IEnumerable<WhatSAP.Models.Activity>

@{
    ViewData["Title"] = "Search";
}

<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap core CSS -->
    <link href="../../dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="offcanvas.css" rel="stylesheet">

</head>
<body>


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

    <!-- Isolated Version of Bootstrap, not needed if your site already uses Bootstrap -->
    <link rel="stylesheet" href="https://formden.com/static/cdn/bootstrap-iso.css" />

    <!-- Bootstrap Date-Picker Plugin -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/js/bootstrap-datepicker.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/css/bootstrap-datepicker3.css" />
    <br />


    <nav class="navbar navbar-expand-lg navbar-light bg-light">

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Date <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Price</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Categories
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="#">Action</a>
                        <a class="dropdown-item" href="#">Another action</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Something else here</a>
                    </div>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
            </form>
        </div>
    </nav>


    <main role="main" class="container">

        <br />
        <div class="row mb-2">
            <div class="col-md-3">
                <div class="card flex-md-row mb-4 shadow-sm h-md-250">
                    <div class="card-body d-flex flex-column align-items-start">
                        <strong class="text-primary">Sports Class</strong>
                    </div>
                    <img class="card-img-right flex-auto d-none d-lg-block" src="~/images/activities/Sportsclass.jpg" alt="Card image cap" style="height:70px">
                </div>
            </div>
            <div class="col-md-3">
                <div class="card flex-md-row mb-4 shadow-sm h-md-250">
                    <div class="card-body d-flex flex-column align-items-start">
                        <strong class="text-primary">Lesson&Class</strong>
                    </div>
                    <img class="card-img-right flex-auto d-none d-lg-block" src="~/images/activities/Craftclass.jpg" alt="Card image cap" style="height:70px">
                </div>
            </div>
            <div class="col-md-3">
                <div class="card flex-md-row mb-4 shadow-sm h-md-250">
                    <div class="card-body d-flex flex-column align-items-start">
                        <strong class="text-primary">Indoor activity</strong>
                    </div>
                    <img class="card-img-right flex-auto d-none d-lg-block" src="~/images/activities/Indoor.jpg" alt="Card image cap" style="height:70px">
                </div>
            </div>
            <div class="col-md-3">
                <div class="card flex-md-row mb-4 shadow-sm h-md-250">
                    <div class="card-body d-flex flex-column align-items-start">
                        <strong class="text-primary">Outdoor activity</strong>
                    </div>
                    <img class="card-img-right flex-auto d-none d-lg-block" src="~/images/activities/Outdoor.jpg" alt="Card image cap" style="height:70px">
                </div>
            </div>
        </div>





        <div class="my-3 p-3 bg-white rounded shadow-sm">
            <h6 class="border-bottom border-gray pb-2 mb-0">Search Result</h6>


            <div class="media text-muted pt-3">
                <div>
                    <img src="~/images/activities/bookclub-2018-10-25.jpg" alt="" class="mr-2 rounded" style="height:50px">
                </div>
                <div>
                    <p class="media-body pb-3 mb-0 small lh-125">

                        <strong class="d-block text-gray-dark">Book Club</strong>
                        Read a book, and then discuss and review it with other people
                    </p>

                </div>

            </div>
            <hr style="color:black" />
            <div class="media text-muted pt-3">
                <div>
                    <img src="~/images/activities/salsadance-2018-10-30.jpg" alt="" class="mr-2 rounded" style="height:50px">
                </div>
                <div>
                    <p class="media-body pb-3 mb-0 small lh-125">

                        <strong class="d-block text-gray-dark">Salsa Dance</strong>
                        If you want to Challenge yourself and take your dancing a notch higher join our  Salsa Dance Club
                    </p>

                </div>

            </div>
            <hr style="color:black" />
            <div class="media text-muted pt-3">
                <div>
                    <img src="~/images/activities/aquariumtour-2018-11-01.jpg" alt="" class="mr-2 rounded" style="height:50px">
                </div>
                <div>
                    <p class="media-body pb-3 mb-0 small lh-125">

                        <strong class="d-block text-gray-dark">Aquarium Tour</strong>
                        Are you interested in marine mammals? Join our Aquarium Tour and have a whole new experience
                    </p>

                </div>

            </div>
            <hr style="color:black" />
            <div class="media text-muted pt-3">
                <div>
                    <img src="~/images/activities/aquariumtour-2018-11-01.jpg" alt="" class="mr-2 rounded" style="height:50px">
                </div>
                <div>
                    <p class="media-body pb-3 mb-0 small lh-125">

                        <strong class="d-block text-gray-dark">Snorkeling</strong>
                        Snorkeling is the practice of swimming on or through a body of water while equipped with a diving mask, a shaped breathing tube called a snorkel, and usually swimfins.
                    </p>

                </div>

            </div>
            <hr style="color:black" />

            <div class="media text-muted pt-3">
                <div>
                    <img src="~/images/activities/aquariumtour-2018-11-01.jpg" alt="" class="mr-2 rounded" style="height:50px">
                </div>
                <div>
                    <p class="media-body pb-3 mb-0 small lh-125">

                        <strong class="d-block text-gray-dark">Pilates Class</strong>
                        Pilates is a physical fitness system developed in the early 20th century by Joseph Pilates, after whom it was named.
                    </p>

                </div>

            </div>
            <hr style="color:black" />
        </div>

    </main>



</body>
</html>
